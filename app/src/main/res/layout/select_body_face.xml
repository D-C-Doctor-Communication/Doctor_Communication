<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto">

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Leftguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.07" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Rightguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.93" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Upguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.03" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Downguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.97" />

<TextView
    android:id="@+id/pattern_text1"
    android:layout_width="0dp"
    android:layout_height="45dp"
    android:layout_marginTop="100dp"
    android:gravity="center"
    android:hint="부위를 선택해주세요"
    android:fontFamily="@font/nanumsquareregular"
    android:textSize="25sp"
    app:layout_constraintBottom_toBottomOf="@id/Downguideline"
    app:layout_constraintEnd_toEndOf="@id/Rightguideline"
    app:layout_constraintHorizontal_bias="0.466"
    app:layout_constraintStart_toStartOf="@id/Leftguideline"
    app:layout_constraintTop_toTopOf="@id/Upguideline"
    app:layout_constraintVertical_bias="0" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginTop="68dp"
        android:layout_marginBottom="68dp"
        android:scaleType="centerInside"
        android:src="@drawable/area_face_line_off"
        app:layout_constraintBottom_toTopOf="@id/nextpage"
        app:layout_constraintEnd_toEndOf="@id/Rightguideline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/Leftguideline"
        app:layout_constraintTop_toBottomOf="@id/pattern_text1"
        app:layout_constraintVertical_bias="0" />


    <ImageButton
        android:id="@+id/face01"
        android:layout_width="27.6dp"
        android:layout_height="72.2dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked01"
        android:layout_marginTop="138.6dp"
        android:layout_marginLeft="57.7dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/face02"
        android:layout_width="27.6dp"
        android:layout_height="71.2dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked02"
        android:layout_marginTop="139.1dp"
        android:layout_marginLeft="265.2dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/face03"
        android:layout_width="57.8dp"
        android:layout_height="44.8dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked03"
        android:layout_marginTop="151.3dp"
        android:layout_marginLeft="97.4dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/face04"
        android:layout_width="57.8dp"
        android:layout_height="44.8dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked04"
        android:layout_marginTop="151.3dp"
        android:layout_marginLeft="194.7dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/face05"
        android:layout_width="34.9dp"
        android:layout_height="68dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked05"
        android:layout_marginTop="164.3dp"
        android:layout_marginLeft="157.6dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/face06"
        android:layout_width="126dp"
        android:layout_height="47.3dp"
        android:scaleType="centerInside"
        android:background="@drawable/face_clicked06"
        android:layout_marginTop="271.4dp"
        android:layout_marginLeft="112dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

<ImageButton
    android:id="@+id/nextpage"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_margin="0dp"
    android:background="@android:color/transparent"
    android:src="@drawable/gotonext"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:scaleType="fitCenter"
    app:layout_constraintHeight_percent="0.13"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.29" />
<ImageButton
    android:id="@+id/backpage"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_margin="0dp"
    android:background="@android:color/transparent"
    android:src="@drawable/gotoback"
    android:scaleType="fitCenter"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHeight_percent="0.13"
    app:layout_constraintHorizontal_bias="0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.29" />

</androidx.constraintlayout.widget.ConstraintLayout>