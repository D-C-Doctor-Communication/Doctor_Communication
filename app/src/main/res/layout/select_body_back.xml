<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto">

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Leftguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.07" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Rightguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.93" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Upguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.03" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Downguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.97" />

<TextView
    android:id="@+id/pattern_text1"
    android:layout_width="0dp"
    android:layout_height="45dp"
    android:layout_marginTop="100dp"
    android:gravity="center"
    android:hint="부위를 선택해주세요"
    android:fontFamily="@font/nanumsquareregular"
    android:textSize="25sp"
    app:layout_constraintBottom_toBottomOf="@id/Downguideline"
    app:layout_constraintEnd_toEndOf="@id/Rightguideline"
    app:layout_constraintHorizontal_bias="0.466"
    app:layout_constraintStart_toStartOf="@id/Leftguideline"
    app:layout_constraintTop_toTopOf="@id/Upguideline"
    app:layout_constraintVertical_bias="0" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginTop="68dp"
        android:layout_marginBottom="68dp"
        android:scaleType="centerInside"
        android:src="@drawable/area_back_line_off"
        app:layout_constraintBottom_toTopOf="@id/nextpage"
        app:layout_constraintEnd_toEndOf="@id/Rightguideline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/Leftguideline"
        app:layout_constraintTop_toBottomOf="@id/pattern_text1"
        app:layout_constraintVertical_bias="0" />

    <ImageButton
        android:id="@+id/back05"
        android:layout_width="186.4dp"
        android:layout_height="235.8dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked05"
        android:layout_marginTop="80.7dp"
        android:layout_marginLeft="81.8dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/back01"
        android:layout_width="84.1dp"
        android:layout_height="62.8dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked01"
        android:layout_marginTop="30.1dp"
        android:layout_marginLeft="85.6dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/back02"
        android:layout_width="84.1dp"
        android:layout_height="62.8dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked02"
        android:layout_marginTop="30.1dp"
        android:layout_marginLeft="181.2dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/back03"
        android:layout_width="102.4dp"
        android:layout_height="89.9dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked03"
        android:layout_marginTop="80.7dp"
        android:layout_marginLeft="38dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/back04"
        android:layout_width="102.4dp"
        android:layout_height="89.9dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked04"
        android:layout_marginTop="80.7dp"
        android:layout_marginLeft="210.6dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />



    <ImageButton
        android:id="@+id/back06"
        android:layout_width="67.4dp"
        android:layout_height="89.3dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked06"
        android:layout_marginTop="224.8dp"
        android:layout_marginLeft="89.5dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/back07"
        android:layout_width="67.4dp"
        android:layout_height="89.3dp"
        android:scaleType="centerInside"
        android:background="@drawable/back_clicked07"
        android:layout_marginTop="224.8dp"
        android:layout_marginLeft="194dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/nextpage"
        android:layout_width="76dp"
        android:layout_height="66dp"
        android:layout_marginBottom="32dp"
        android:background="@android:color/transparent"
        android:src="@drawable/gotonext"
        android:scaleType="fitEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/backpage"
        android:layout_width="76dp"
        android:layout_height="66dp"
        android:layout_marginBottom="32dp"
        android:background="@android:color/transparent"
        android:src="@drawable/gotoback"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>