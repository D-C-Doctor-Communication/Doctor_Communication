<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto">

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Leftguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.07" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Rightguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.93" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Upguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.03" />

<androidx.constraintlayout.widget.Guideline
    android:id="@+id/Downguideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.97" />

<TextView
    android:id="@+id/pattern_text1"
    android:layout_width="0dp"
    android:layout_height="45dp"
    android:layout_marginTop="100dp"
    android:gravity="center"
    android:hint="부위를 선택해주세요"
    android:fontFamily="@font/nanumsquareregular"
    android:textSize="25sp"
    app:layout_constraintBottom_toBottomOf="@id/Downguideline"
    app:layout_constraintEnd_toEndOf="@id/Rightguideline"
    app:layout_constraintHorizontal_bias="0.466"
    app:layout_constraintStart_toStartOf="@id/Leftguideline"
    app:layout_constraintTop_toTopOf="@id/Upguideline"
    app:layout_constraintVertical_bias="0" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginTop="68dp"
        android:layout_marginBottom="68dp"
        android:scaleType="centerInside"
        android:src="@drawable/area_head_line"
        app:layout_constraintBottom_toTopOf="@id/nextpage"
        app:layout_constraintEnd_toEndOf="@id/Rightguideline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/Leftguideline"
        app:layout_constraintTop_toBottomOf="@id/pattern_text1"
        app:layout_constraintVertical_bias="0" />

    <ImageButton
        android:id="@+id/head04"
        android:layout_width="240dp"
        android:layout_height="148dp"
        android:scaleType="centerInside"
        android:background="@drawable/head_clicked04"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintVertical_bias="0.143"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/head05"
        android:layout_width="90dp"
        android:layout_height="145dp"
        android:scaleType="centerInside"
        android:background="@drawable/head_clicked05"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintVertical_bias="0.62"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/head01"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:scaleType="centerInside"
        android:background="@drawable/head_clicked01"
        app:layout_constraintHorizontal_bias="0.23"
        app:layout_constraintVertical_bias="0.46"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />

    <ImageButton
        android:id="@+id/head02"
        android:layout_width="98dp"
        android:layout_height="45dp"
        android:scaleType="centerInside"
        android:background="@drawable/head_clicked02"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintVertical_bias="0.319"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />
    <ImageButton
        android:id="@+id/head03"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:scaleType="centerInside"
        android:background="@drawable/head_clicked03"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintVertical_bias="0.46"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintEnd_toEndOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />



<ImageButton
    android:id="@+id/nextpage"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_margin="0dp"
    android:background="@android:color/transparent"
    android:src="@drawable/gotonext"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:scaleType="fitCenter"
    app:layout_constraintHeight_percent="0.13"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.29" />
<ImageButton
    android:id="@+id/backpage"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_margin="0dp"
    android:background="@android:color/transparent"
    android:src="@drawable/gotoback"
    android:scaleType="fitCenter"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHeight_percent="0.13"
    app:layout_constraintHorizontal_bias="0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.29" />

</androidx.constraintlayout.widget.ConstraintLayout>